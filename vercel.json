{
  "version": 2,
  "name": "crwd-platform",
  "builds": [
    {
      "src": "api/backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/backend/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}